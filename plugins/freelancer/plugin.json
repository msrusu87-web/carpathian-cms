    "Sistem de Gig-uri (servicii)",
    "Management comenzi",
    "Sistem de mesagerie",
    "Tracking câștiguri",
    "Rating și review-uri",
    "Frontend menu automat"
  ],
  "config": {
    "frontend_menus": [
      {
        "name": "Freelancer",
        "slug": "freelancer-menu",
        "location": "header",
        "items": [
          {"title": "Freelancer", "url": "/freelancer", "icon": "briefcase"},
          {"title": "Găsește Freelanceri", "url": "/freelancer/profiles", "icon": "users"},
          {"title": "Servicii (Gigs)", "url": "/freelancer/gigs", "icon": "star"},
          {"title": "Comenzi Active", "url": "/freelancer/orders", "icon": "shopping-cart"},
          {"title": "Devino Freelancer", "url": "/freelancer/become", "icon": "rocket"}
        ]
      }
    ]
  }
}
